
# Memoization
class Solution {
    public int climbStairs(int n) {
    HashMap <Integer, Integer> map = new HashMap <Integer, Integer>();    
    
      return helper(n, map);
    }
    
    private int helper (int n, HashMap <Integer, Integer> map ){ 
        if (n < 0)  return 0;
        if (n <=1 ) return 1;
        
        if (map.containsKey(n)) {
            return map.get(n);
        }
        else {
            map.put(n, helper(n-1, map) + helper (n-2, map));
        }

        return map.get(n);
    }
}
